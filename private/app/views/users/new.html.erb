<section class="sign-up">

      <h2>SingUp</h2>
      
      <%= form_for(@user, url: users_path) do |f| %>
            <%= f.label :name %>
            <%= f.text_field :name %>

            <%= f.label :email %>
            <%= f.email_field :email , autofocus: true, autocomplete: "email" %>

            <%= f.submit "Sign up" %>
      <% end %>

</section> 