<section class="sign-up">
<h1>SIGN UP</h1>
<%#= render 'shared/error_messages' %>
<%= form_for(@user, url: users_path) do |f| %>
      <%= f.label :name %>
      <%= f.text_field :name %>

      <%= f.label :email %>
      <%= f.email_field :email %>

      <%= f.submit "Create my account" %>
<% end %>
</section> 